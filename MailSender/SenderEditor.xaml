<Window x:Class="MailSender.SenderEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MailSender"
        mc:Ignorable="d"
        Title="SenderEditor" Height="300" Width="330">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center">Имя отправителя</TextBlock>
            <TextBox x:Name="editSenderName" Text=""></TextBox>
            <TextBlock HorizontalAlignment="Center">Пароль</TextBlock>
            <PasswordBox x:Name="editSenderPassword"></PasswordBox>
        </StackPanel>
    </Grid>
</Window>
